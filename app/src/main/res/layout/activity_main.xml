<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Load"
        android:id="@+id/load.b"
        android:layout_alignParentStart="true"
        android:layout_marginTop="46dp"
        android:clickable="true"
        android:enabled="true"
        android:onClick="clickLoad" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New"
        android:id="@+id/new.b"
        android:layout_below="@+id/quit.b"
        android:layout_alignParentStart="true"
        android:clickable="true"
        android:enabled="true"
        android:onClick="clickNew" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Quit"
        android:id="@+id/quit.b"
        android:layout_below="@+id/load.b"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="61dp"
        android:clickable="true"
        android:enabled="true"
        android:onClick="clickQuit" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Demographics"
        android:id="@+id/demographics.b"
        android:layout_below="@+id/new.b"
        android:layout_toStartOf="@+id/quit.b"
        android:layout_marginTop="76dp"
        android:visibility="invisible" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Questions"
        android:id="@+id/questions.b"
        android:layout_below="@+id/demographics.b"
        android:layout_alignStart="@+id/demographics.b"
        android:layout_marginTop="41dp"
        android:visibility="invisible" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Analysis"
        android:id="@+id/analysis.b"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/questions.b"
        android:visibility="invisible" />
</RelativeLayout>
